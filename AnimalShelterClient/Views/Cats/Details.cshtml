@{
  ViewData["Title"] = $"Details for {Model.Name}";
}

<h1>All About @Model.Name</h1>

<p>Breed: @Model.Breed</p>
<p>Age: @Model.Age</p>



<p>@Html.ActionLink("Edit cat details", "Edit", new { id = @Model.CatId })</p>
<p>@Html.ActionLink("Remove cat from shelter", "Delete", new { id = @Model.CatId })</p>
@Html.ActionLink("Return to all cats", "Index")